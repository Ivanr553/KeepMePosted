{
  "name": "keep-me-posted",
  "version": "1.0.0",
  "description": "A website application to allow users to track their waitlisted classes and to be notified when they are open through text message or email.",
  "main": "node bin/www",
  "scripts": {
    "start": "node bin/www",
    "build": "webpack --config webpack.build.js",
    "webpackWatch": "webpack --watch --config webpack.dev.js",
    "serve": "nodemon bin/www",
    "dev": "npm-run-all --parallel webpackWatch serve"
  },
  "author": "Ivan",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.1",
    "file-loader": "^1.1.5",
    "fs": "0.0.1-security",
    "mongoose": "^4.12.0",
    "path": "^0.12.7",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "image-webpack-loader": "^3.4.2",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "webpack": "^3.6.0"
  },
  "engines": {
    "node": "v8.6.0"
  }
}
